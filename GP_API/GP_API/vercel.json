{
	"rewrites":[
      {
         "source": "/auth/(.*)",
         "destination": "/auth/auth.py"
      },
      {
         "source": "/errors/(.*)",
         "destination": "/errors/errors.py"
      },
      {
         "source": "/jsoncheck/(.*)",
         "destination": "/jsoncheck/json_checker.py"
      },
      {
         "source": "/(.*)",
         "destination": "/api/main.py"
      }
	]
}